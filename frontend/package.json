{
  "name": "medvisor-frontend",
  "version": "1.0.0",
  "description": "MedVisor Frontend - Medical visualization interface",
  "private": true,
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
